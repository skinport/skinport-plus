{
  "manifest_version": 3,
  "name": "Skinport",
  "version": "1.0.0",
  "description": "Skinport extension",
  "host_permissions": ["https://api.skinport.com/*"],
  "content_scripts": [
    {
      "matches": ["https://steamcommunity.com/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    }
  ]
}
